{
  "name": "serve",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/build/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ]
  },
  "scripts": {
    "build": "tsc ",
    "setup:database": "docker compose up -d",
    "dev": "concurrently \"tsc --watch\" \"nodemon ./build/server.js\"",
    "serve": "npm run setup:database && tsc && node ./build/server.js",
    "test": "tsc && jest"
  },
