    "pg": "^8.7.3",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.17.12",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.42",
    "concurrently": "^8.2.1",
